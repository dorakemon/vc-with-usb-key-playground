{
  "name": "vc-holder-binding-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check": "run-p check:lint check:type",
    "check:lint": "npx biome check ./src",
    "check:fix": "npx biome check --write ./src",
    "check:fix-unsafe": "npx biome check --write --unsafe ./src",
    "check:type": "tsc",
    "upgrade-deps": "npm-check-updates -u && npm install"
  },
  "dependencies": {
    "next": "15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "npm-check-updates": "^17.1.13",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "volta": {
    "node": "22.12.0",
    "npm": "11.0.0"
  }
}
